{
    "$schema": "https://mintlify.com/docs.json",
    "theme": "mint",
    "name": "Mint Starter Kit",
    "appearance": {
        "default": "dark",
        "strict": true
    },
    "colors": {
        "primary": "#16A34A",
        "light": "#07C983",
        "dark": "#15803D"
    },
    "favicon": "/favicon.ico",
    "navigation": {
        "tabs": [
            {
                "tab": "Guides",
                "groups": [
                    {
                        "group": "Get Started",
                        "pages": [
                            "index",
                            "quickstart",
                            "development"
