                {
                    "anchor": "Discover Agents",
                    "href": "https://directory.bhindi.io/",
                    "icon": "magnifying-glass"
                },
                {
                    "anchor": "Join Community",
                    "href": "https://discord.gg/hSfTG33ymy",
                    "icon": "discord"
                }
            ]
        }
    },
    "logo": {
        "light": "/logo/bhindi-logo.webp",
        "dark": "/logo/bhindi-logo.webp"
    },
    "navbar": {
        "primary": {
            "type": "button",
            "label": "Launch Bhindi",
